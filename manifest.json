{
    "manifest_version": 3,
    "name": "Keyword Blocker",
    "description": "Block pages containing specified keywords.",
    "version": "1.0.0",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "Logo.png"
    },
    "icons": {
      "128": "Logo128.png"
    }
  }
  